    <script>
        (function ($) {
            "use strict"

            var dzMorris = function () {

                var screenWidth = $(window).width();

                var setChartWidth = function () {
                    if (screenWidth <= 768) {
                        var chartBlockWidth = 0;
                        chartBlockWidth = (screenWidth < 300) ? screenWidth : 300;
                        jQuery('.morris_chart_height').css('min-width', chartBlockWidth - 31);
                    }
                }

          
                var barChart = function () {
                    if (jQuery('#morris_bar').length > 0) {
                        debugger;
                        $.ajax({
                            url: "/Franchise/GetChartFranchiseSaleRequest",
                            type: "GET",
                            dataType: 'JSON',
                            data: JSON.stringify(),
                            contentType: 'application/json;',
                            success: function (d) {
                                debugger;
                                var TotalSaleRequest = [];
                                var TotalApproved = [];
                                var TotalRejected = [];
                                var TotalPending = [];
                                var AllYear = [];
                                var t = JSON.parse(JSON.stringify(d));
                                for (var i = 0; i < t.length; i++) {
                                    AllYear.push(t[i].Year);
                                    TotalSaleRequest.push(t[i].TotalSaleRequest);
                                    TotalApproved.push(t[i].TotalApproved);
                                    TotalRejected.push(t[i].TotalRejected);
                                    TotalPending.push(t[i].TotalPending);
                                    debugger;
                                    var xs = AllYear;
                                    var ys = 'Total Sale Request(MTG)- '+parseFloat(TotalSaleRequest);
                                    var ya = parseFloat(TotalApproved);
                                    var yr = parseFloat(TotalRejected);
                                    var yp = parseFloat(TotalPending);
                                    Morris.Bar({
                                        element: 'morris_bar',
                                        data: [{
                                            y: ys,
                                            a: ya,
                                            b: yr,
                                            c: yp
                                        }],

                                        xkey: 'y',
                                        ykeys: ['a', 'b', 'c'],
                                        labels: ['Total Approve(MTG)', 'Total Rejected(MTG)', 'Total Pending(MTG)'],
                                        barColors: ['#363062', '#D8B9C3', '#ff9f00'],
                                        hideHover: 'auto',
                                        gridLineColor: 'transparent',
                                        resize: true,
                                        barSizeRatio: 0.45,
                                    });
                                }
                            }
                        })
                    }
                }


                /* Function ============ */
                return {
                    init: function () {
                        setChartWidth();
                        barChart();
                    },


                    resize: function () {
                        screenWidth = $(window).width();
                        setChartWidth();
                        barChart();
                    }
                }

            }();

            jQuery(document).ready(function () {
                dzMorris.init();
            });



        })(jQuery);
    </script>
