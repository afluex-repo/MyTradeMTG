@model MyTradeMTG.Models.UserReports
@{
    ViewBag.Title = "LevelIncomeTr1Total";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

@using (Html.BeginForm("LevelIncomeTr1Total", "Report", FormMethod.Post, new { @id = "form1", enctype = "multipart/form-data" }))
{
    <div class="content-body">
        <div class="container-fluid">
            <div class="row layout-top-spacing">
                <div class="col-lg-12 col-12 layout-spacing">
                    <div class="card">
                        <div class="card-header">
                            <div class="row">
                                <div class="col-xl-12 col-md-12 col-sm-12 col-12">
                                    <h4>Level Bonus TR1</h4>
                                </div>
                            </div>
                        </div>
                        <div class="card-body">
                            <div class="row">
                                <table id="html5-extension" class="table table-hover non-hover" data-page-length='20'>
                                    <thead>
                                        <tr>
                                            <th>Sr No.</th>
                                            <th>Business MTG Coin</th>
                                            <th>Income</th>
                                            <th>Level</th>
                                            <th>Status</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model != null)
                                        {
                                            if (Model.lst != null)
                                            {
                                                int i = 1;
                                                foreach (var Item in Model.lst)
                                                {
                                                    <tr class="text-left">
                                                        <td>@i</td>
                                                        <td>@Item.BusinessAmount</td>
                                                        <td>@Item.Amount</td>
                                                        <td><a href="/Report/LevelIncomeTr1?Lvl=@Item.Level" class="btn btn-info">@Item.Level</a></td>
                                                        <td>@Item.Status</td>
                                                    </tr>
                                                    i++;
                                                }
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </div>
        }

